<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.audit.modules.report.dao.SingleCarrierChargeDao">
  <resultMap id="BaseResultMap" type="com.audit.modules.report.entity.SingleCarrierCharge">
    <id column="ID" jdbcType="DECIMAL" property="id" />
    <result column="CITY_ID" jdbcType="VARCHAR" property="cityId" />
    <result column="COUNTY_ID" jdbcType="VARCHAR" property="countyId" />
    <result column="REGION_NAME" jdbcType="VARCHAR" property="regionName" />
    <result column="CREATE_TIME" jdbcType="TIMESTAMP" property="createTime" />
    <result column="SCEC_CHARGE" jdbcType="VARCHAR" property="scecCharge" />
    <result column="SINGLE_MONEY" jdbcType="VARCHAR" property="singleMoney" />
    <result column="TYPE_CODE" jdbcType="VARCHAR" property="typeCode" />
  </resultMap>
  <sql id="Base_Column_List">
    ID, CITY_ID, COUNTY_ID, REGION_NAME, CREATE_TIME, SCEC_CHARGE, TYPE_CODE,SINGLE_MONEY
  </sql>
  <select id="findByCodeTypeAndYear"  resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from SYS_STASTIC_SCEC
    where TYPE_CODE = #{typeCode,jdbcType=VARCHAR}
     AND YEAR = #{year,jdbcType=VARCHAR}
  </select>
</mapper>